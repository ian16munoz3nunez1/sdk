'etc/posj.autoconfig.FiscalPrinter.properties':'''\
# Properties for FiscalPrinter

# See "User's Guide" for a general description of this file.

# This will override the automatically-generated logical name for the corresponding device.
#logicalName.1=FiscalPrinter1


# These are RS232 properties.  These should only be used if the device
# is directly connected to the RS232 port, not for subdevices.

# This specifies the device is connected via the RS232 bus.
# This must be specified for a RS232-connected device.
# The only allowable value is "RS232", no other value should be used.
# For devices that are not RS232-connected, this should not be specified.
#deviceBus.1=RS232

# This specifies the port that the device is connected to.
# This must be specified for a RS232-connected device.
# The value should be the RS232 port name in DOS nomenclature, i.e. "COM1", "COM2", etc.
# Note that "COM1" is the first RS232 port, there is no "COM0".
#portName.1=COM1

# This specifies the baud rate that the device uses.
# This will default to a predetermined value if not specified.
# The value should be the baud rate as a decimal number.
# This baudRate is the default 
#baudRate.1=19200
# The FiscalPrinter model 4610-Sx6 must set the baudRate to 115200 to work
#baudRate.1=115200

''',

'etc/posj.autoconfig.CheckScanner.properties':'''\
# Properties for CheckScanner

# See "User's Guide" for a general description of this file.

# This will override the automatically-generated logical name for the corresponding device.
#logicalName.1=CheckScanner1
''',

'etc/posj.autoconfig.BillDispenser.properties':'''\
# Properties for BillDispenser

# See "User's Guide" for a general description of this file.

# This will override the automatically-generated logical name for the corresponding device.
#logicalName.1=BillDispenser1
'''

}


# This function may be used to create required files and
# directories
def createFiles(contentDict):

    if not isinstance(contentDict, dict):
        raise Exception("The createFiles function requires a dictionary argument")

    os.chdir("/opt/tgcs/javapos")

    for pathName in list(contentDict):
        dirName = os.path.dirname(pathName)
        if os.path.exists(dirName):
          pass
        else:
          os.makedirs(dirName, 0750)
        with open(pathName, "w") as f:
          f.write(contentDict[pathName])

    keystore = "/mnt/f/adxetc/ext/adxqrsdk/conf/keystore"
    if not os.path.isdir(keystore):
        os.makedirs(keystore)

    passwd = "tcxsdk"

    # ca
    cmd = "keytool -genkeypair -alias sdk-ca -keyalg RSA -keysize 2048 -dname \"CN=localhost, O=Toshiba Global Commerce Solutions, OU=SDK development, L=Zapopan, ST=Jalisco, C=MX\" -keystore \"" + os.path.join(keystore, "sdk-ca.jks") + "\" -storepass " + passwd + " -keypass " + passwd +" -noprompt"
    os.system(cmd)
    cmd = "keytool -exportcert -alias sdk-ca -keystore \"" + os.path.join(keystore, "sdk-ca.jks") + "\" -file \"" + os.path.join(keystore, "sdk-ca.cer") + "\" -storepass " + passwd + " -noprompt"
    os.system(cmd)
    cmd = "keytool -importkeystore -srckeystore \"" + os.path.join(keystore, "sdk-ca.jks") + "\" -destkeystore \"" + os.path.join(keystore, "sdk-ca.p12") + "\" -deststoretype PKCS12 -srcstorepass " + passwd + " -deststorepass " + passwd + " -noprompt"
    os.system(cmd)
    cmd = "openssl pkcs12 -in \"" + os.path.join(keystore, "sdk-ca.p12") + "\" -nocerts -nodes -out \"" + os.path.join(keystore, "sdk-ca.key") + "\" -passin pass:" + passwd
    os.system(cmd)
    cmd = "openssl rsa -in \"" + os.path.join(keystore, "sdk-ca.key") + "\" -out \"" + os.path.join(keystore, "sdk-ca.pem") + "\""
    os.system(cmd)

    # client
    cmd = "keytool -genkeypair -alias sdk-client -keyalg RSA -keysize 2048 -dname \"CN=localhost, O=Toshiba Global Commerce Solutions, OU=SDK development, L=Zapopan, ST=Jalisco, C=MX\" -keystore \"" + os.path.join(keystore, "sdk-client.jks") + "\" -storepass " + passwd + " -keypass " + passwd +" -noprompt"
    os.system(cmd)
    cmd = "keytool -exportcert -alias sdk-client -keystore \"" + os.path.join(keystore, "sdk-client.jks") + "\" -file \"" + os.path.join(keystore, "sdk-client.cer") + "\" -storepass " + passwd + " -noprompt"
    os.system(cmd)
    cmd = "keytool -importkeystore -srckeystore \"" + os.path.join(keystore, "sdk-client.jks") + "\" -destkeystore \"" + os.path.join(keystore, "sdk-client.p12") + "\" -deststoretype PKCS12 -srcstorepass " + passwd + " -deststorepass " + passwd + " -noprompt"
    os.system(cmd)
    cmd = "openssl pkcs12 -in \"" + os.path.join(keystore, "sdk-client.p12") + "\" -nocerts -nodes -out \"" + os.path.join(keystore, "sdk-client.key") + "\" -passin pass:" + passwd
    os.system(cmd)
    cmd = "openssl rsa -in \"" + os.path.join(keystore, "sdk-client.key") + "\" -out \"" + os.path.join(keystore, "sdk-client.pem") + "\""
    os.system(cmd)

    # server
    cmd = "keytool -genkeypair -alias sdk-wss -keyalg RSA -keysize 2048 -dname \"CN=localhost, O=Toshiba Global Commerce Solutions, OU=SDK development, L=Zapopan, ST=Jalisco, C=MX\" -keystore \"" + os.path.join(keystore, "sdk-wss.jks") + "\" -storepass " + passwd + " -keypass " + passwd +" -noprompt"
    os.system(cmd)
    cmd = "keytool -exportcert -alias sdk-wss -keystore \"" + os.path.join(keystore, "sdk-wss.jks") + "\" -file \"" + os.path.join(keystore, "sdk-wss.cer") + "\" -storepass " + passwd + " -noprompt"
    os.system(cmd)
    cmd = "keytool -importkeystore -srckeystore \"" + os.path.join(keystore, "sdk-wss.jks") + "\" -destkeystore \"" + os.path.join(keystore, "sdk-wss.p12") + "\" -deststoretype PKCS12 -srcstorepass " + passwd + " -deststorepass " + passwd + " -noprompt"
    os.system(cmd)
    cmd = "openssl pkcs12 -in \"" + os.path.join(keystore, "sdk-wss.p12") + "\" -nocerts -nodes -out \"" + os.path.join(keystore, "sdk-wss.key") + "\" -passin pass:" + passwd
    os.system(cmd)
    cmd = "openssl rsa -in \"" + os.path.join(keystore, "sdk-wss.key") + "\" -out \"" + os.path.join(keystore, "sdk-wss.pem") + "\""
    os.system(cmd)


if __name__ == "__main__":
    createFiles(defaults)
