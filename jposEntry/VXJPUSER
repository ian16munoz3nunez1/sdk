#---------------------------------------------------------------------------------------------------
# Scanner Settings
#

# Scanner protocol converters product IDs
# Used to submit a OEM Scanner reset command at population time.
# Property format: ...oemScannerProtocols.<ID number> = <vendorID>,<productID>
#
com.ibm.posj.bus.hid.javaxusb.factory.oemScannerProtocols.1 = 0x0886,0x9900

#
# Time out ms value to use for Scanner commands.
#
com.ibm.posj.bus.ScannerHandleImp.COMMAND_TIMEOUT = 1500

#
# Synchronous Command Submission
#
# Description
# Use this property to enable/disable Synchronous hardware command submission. 
# When enabled the driver will wait for the hardware to process notifications.
#
# Valid Values:
#    true   Synchronous command submission enabled (default).
#    false  Asynchronous command submission enabled.
#
# com.ibm.posj.bus.rs485.Rs485Scanner.SynchronousSubmission = true

#
# Scanner Type and Response Length
# Use this settings to force the scanner to use a specific command set, by default it
# uses the type identified by POSS
# Property format: ...Rs485ScannerHandleImp.<port> = <scannerType>
#     ScannerType: 
#         - 1520 ( 1 byte command, 3 byte of status response )
#         - 4500 ( 2 byte command, 2 byte of status response )
#         - 4501 ( 2 byte command, 2 byte of status response )
#         - 4696 ( 1 byte command, 3 byte of status response )
#         - 4697 ( 1 byte command, 3 byte of status response )
#         - 4698 ( 1 byte command, 3 byte of status response )
#         - OEM1 ( 1 byte command, 3 byte of status response)
#         - OEM2 ( 2 byte command, 2 byte of status response)
#com.ibm.posj.bus.rs485.Rs485ScannerHandleImp.ScannerType.4A = OEM1
#com.ibm.posj.bus.rs485.Rs485ScannerHandleImp.ScannerType.4B = OEM2

#
# Scanner Datalogic management infrastructure
# Used to enable the OEM Scanner Management functionality for RMA.
# Property format: ...datalogicScannerMgmt = <boolean>
#
# Values accepted : true or false(default)
#
# Since 1.14.0
#
#com.ibm.posj.bus.hid.javaxusb.factory.datalogicScannerMgmt = false

#
# Time for DirectIO for Command SCANNER_ID
# This property was created to have the option to change the time 
# that we need to wait a response for scanner, using DirectIO command
# for SCANNER_ID command only
#
# Values accepted : minimum value = 1000;
#                   maximum value = 30000;
# Since 1.14.3
#
com.ibm.posj.bus.scanner.SCANNER_ID_TIMEOUT = 1000

#
# Scanner Datalogic Data 11 byte Padding Managing
# Used to disable the padding  for Datalogic Scanners
# Property format: ...DATALOGIC_DATAPADDING_ENABLE = <boolean>
#
# Values accepted : true(default) or false
#
# Since 1.14.9
#
com.ibm.posj.bus.scanner.DATALOGIC_DATAPADDING_ENABLE = true

#------------------------------------------------------------------------------
# Lights Settings
#

#
#Timeout used when sending commands to GuidanceLights
#	Use this property to change the time it needs to wait
#	after sending a command to guidance lights
#
# Since 1.14.6
#
com.tgcs.posj.Lights.GuidangeLights.CmdTimeout = 500

#------------------------------------------------------------------------------
# ToneIndicator Settings
#

#
# MinimumToneDurationTo100 property.
# 
# Used to configure the Tone duration behavior. By default when the tone 
# duration is set to a value between 1 and 99 milliseconds the tone indicator 
# will not sound(see Tone1Duration and Tone2Duration in UPOS specification). 
#
# That behavior can be modified. If the tone indicator is required to sound when
# setting a duration value in that range this property should be set to true. 
# That will cause the tone to beep during 100 milliseconds for all values 
# between 1 and 99 milliseconds.
#
# The tone indicator will not sound either way if tone indicator duration is set
# to 0 milliseconds.
#
#  
# Devices Supported:
#                    USB  Modular 67 Key Keyboard with Display
#                    USB  Modular 67 Key Keyboard
#                    PS/2 Modular 67 Key Keyboard
#                    PS/2 Modular Compact Alphanumeric Keyboard
#                    USB  Modular Compact Alphanumeric Keyboard
#                    PS/2 Modular Alphanumeric Keyboard
#                    USB  Modular Alphanumeric Keyboard
#
# Values accepted : true or false(default)
#
# 
# since 1.14.1
#
#com.ibm.posj.ToneIndicator.MinimumToneDurationTo100 = true

# --------------------------------------------------------------------------------------------------
# HID/javax.usb Populator settings
#

#Enables/Disables device serial number/device number binding logic.
#This functionality will be enabled by default when a tablet is detected.
#This functionality will be disabled by default when any system that is not a tablet is detected
#  
# Values Accepted : 
#     - property commented out ( default ):
#                   When the driver detect the running system is a Mobile Tablet it will set the property as true, for all
#                   other systems the property will be set to false.
#     - true  : Will ignore serial numbers for enumeration
#     - false : Will NOT ignore serial numbers ( as 1.13.6 and previous ).
#
# since 1.13.7
#
#com.ibm.posj.bus.hid.javaxusb.factory.ReconnectionIgnoreSerialNumber = true

# 
# Set the Reports attempted length  in HID  Get Report Command.
# Property format: ...AttemptLength.<ID number> = <vendorID>,<productID>,<attempt_length>
#  Example com.ibm.posj.bus.hid.javaxusb.factory.attemptLength.1 = 0x04b3,0x4873,0x122
# com.ibm.posj.bus.hid.javaxusb.factory.reportAttemptLength.1 = 0x04b3,0x4873,0x122

#
# Number of milliseconds to wait while starting populator
#
com.ibm.posj.bus.hid.javaxusb.UsbHandlePopulator.WINDOWS_INIT_DELAY = 0
com.ibm.posj.bus.hid.javaxusb.UsbHandlePopulator.LINUX_INIT_DELAY = 0

#
# Number of milliseconds to wait before to reinitialize USB devices after suspend
#
com.ibm.posj.bus.hid.javaxusb.UsbHandlePopulator.WINDOWS_S3_DELAY = 12000
com.ibm.posj.bus.hid.javaxusb.UsbHandlePopulator.LINUX_S3_DELAY = 0

#
# Number of milliseconds to wait before processing a newly connected device
# (multiply by number of concurrently connected devices)
#
com.ibm.posj.bus.hid.javaxusb.UsbHandlePopulator.CONNECT_DELAY = 100

#
# Maximum number of milliseconds to wait for a device to re-enumerate.
#
com.ibm.posj.bus.hid.javaxusb.UsbHandlePopulator.MAX_REENUMERATION_DELAY = 30000;

#
# Determines if the 4610 per bus will use EC commands.
# To activate EC for a certain bus uncomment that particular line.
#

#rs232.4610.EC
#rs485.4610.EC
#usb.4610.EC
#posj.printer.error.timeout=2000
#Giving apps an option to further limit us ECs, at a risk of early completion.
#LineCount.LOOSE

#
# Determines if the POSPrinter will use line counting.
# To activate LC for a certain bus uncomment that particular line.
#

#rs232.POSPrinter.LC
#rs485.POSPrinter.LC
#usb.POSPrinter.LC

#
# Disable the status for each LineCount increment.
# When the property is defined there will be only one status for every 15 LineCount
# increments instead of one each.
# for a certain bus uncomment that particular line.
#

#rs232.DisableStatusforLineCountInc
#rs485.DisableStatusforLineCountInc
#usb.DisableStatusforLineCountInc

#
# Set the value of the Command Timeout in the printer service  
# Note: Value should be in MILLISECONDS
#
# com.ibm.posj.bus.printer.COMMAND_TIMEOUT = 30800
#
# Set the value of the Offline Timeout for the 4610 Rs232 printers  
# Note: Value should be in MILLISECONDS and the minimum accepted is 5000
#
# com.ibm.posj.bus.rs232.Rs2324610PrinterHandleImp.OFFLINE_TIMEOUT = 12800

#
# Sets the Printer to work in Compatiblity(TI3/4 Emulation) mode when true or in
# Native Mode when false. 
# Devices Supported: 4610-2xR, 6145-2xA
# Note: Values accepted True(Compatibility) and False(Native)
#       Any invalid value will change the printer to Native Mode
#       If the property is disabled, no change will be performed in the printer
com.ibm.posj.POSPrinter.SetCompatibilityMode = false

#
# Sets the Driver to work by allowing multiple JVM  mode when true or Single JVM
#when other values
# Note: Values accepted True(Multiple JVMS) and False (Default Value)(Single JVM)
# Any invalid value will change the printer to work with a Single JVM
#com.tgcs.posj.EnableMultipleJVM = false

#
#com.ibm.posj.printer.concurrent_printing.NetworkIdleTime
#
# Description:
# This property controls the amount of seconds that the Network printer interface 
# will wait after USB print job finishes. Property value represent the number of
# seconds the printer should wait.
#
#Default: No update will be performed.
#Minimum:1
#Maximum: 655
# com.ibm.posj.printer.concurrent_printing.NetworkIdleTime = 1

#
#com.ibm.posj.printer.concurrent_printing.ConcurrencyTimeout
#
# Description:
# This property controls the amount of seconds that JavaPOS driver will wait 
# for Printer hardware to be released of a concurrent job. Property value
# represents the number of seconds the printer will wait.
#
#Default:  30
#Minimum:1
#Maximum: 655
# com.ibm.posj.printer.concurrent_printing.ConcurrencyTimeout = 30

#
# Set the value for the top limit to say a printer has a low
# USB BCD level(invalid level)
# example: 0x0208 means all printer's with a value equal or less
# will be handled as a invalid BCD level.  
# Note: Value should be in hexadecimal representation
com.ibm.posj.bus.hid.printer.INVALID_BCD_LEVEL.1 = 0x0106
com.ibm.posj.bus.hid.printer.INVALID_BCD_LEVEL.2 = 0x0208

#
# Sets the Printer to Change the print order for Compound Vietnamese Characters 
# mode when true or into print normal order when false. 
# Devices Supported: 4610-2xR, 6145-2xA
# Note: Values accepted True(Change order for Compound Vietnamese Order) 
# and False(Normal Order)
#       Any invalid value will change the printer to print Normal Order
#       If the property is disabled, no change will be performed in the printer
#com.ibm.posj.POSPrinter.CompoundVietnameseCharacters = false

