#
# Events port name for CIM Provider.
#
provider.eventSocket.Port=42114

#
# Timeout property for requests from CIM provider to UPOS (default 30000 msecs)
#
provider.response.timeout=30000

#
# Request port name for UPOS 
#
upos.requestSocket.Port=42115

#
# Event Destination IP Address 
#
